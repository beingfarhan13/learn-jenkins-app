{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "learn-jenkins-taskDefinition-prod",
    "containerDefinitions": [
        {
            "name": "myjenkinsapp",
            "image": "853452245552.dkr.ecr.us-east-1.amazonaws.com/myjenkinsapp:#APP_VERSION#",
            "portMappings": [
                {
                    "name": "nginx-80-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "3 GB",
    "cpu": "1 vCPU",
    "executionRoleArn": "arn:aws:iam::853452245552:role/ecsTaskExecutionRole"
}