{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "learn-jenkins-taskDefinition-prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "nginx:1.26-alpine",
            "portMappings": [
                {
                    "name": "nginx-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "3 GB",
    "cpu": "1 vCPU",
    "executionRoleArn": ""
}